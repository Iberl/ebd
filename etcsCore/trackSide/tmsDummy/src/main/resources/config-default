#Config file for TMS_Dummy at EBD
#Lines that start with # are comments
#To expand this file, set variables in ebd.tms_Dummy.handler.ConfigHandler and include variable names here



############# Program Variables #############
######
###### Strings
######
#if no scenario is given by a command line parameter, the following specified scenario will be used
scenario = szenario1.txt
tmsId = 1
rbcId = 1
ipToRBCServer = 127.0.0.1

######
###### Booleans (either " = true" or " = false")
######

# Enables the display and saving of debug information. Changes the logger level from INFO to FINE.
debug = false

# Indicates wether the dummy should continue running the tcp server or should shutdown after all sequences finished.
shutdown = true

######
###### Integers
######
portOfRBCServer = 22224
portOfTMSServer = 22223

######
###### Doubles with a dot, not a comma (1.2 instead of 1,2).
######
# Time acceleration factor. See timeBase under Longs
timeAccFactor = 1

######
###### Longs
######
# Time base in milli seconds since epoch. The default 1593522000000 equals 2020-06-30 15:00:00 UTC+2h
# See timeAccFactor under Doubles
timeBase = 1593522000000